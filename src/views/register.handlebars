<section class="container">
    <h1>{{title}}</h1>
    <form action="/api/sessions/register" method="post">
        <div>
            <div>
                <label for="name">Name</label>
                <br />
                <input id="name" name="name" placeholder="Juan" />
            </div>
            <div>
                <label for="last_name">Last Name</label>
                <br />
                <input id="last_name" name="last_name" placeholder="Perez" />
            </div>
            <div>
                <label for="email">Email</label>
                <br />
                <input type="email" id="email" name="email" placeholder="example@mail.com" />
            </div>
            <div>
                <label for="password">Password</label>
                <br />
                <input type="password" id="password" name="password" placeholder="12345" autocomplete="additional-name"/>
            </div>
            <div>
                <label for="age">Age</label>
                <br />
                <input type="Number" id="age" name="age" placeholder="20" min="1" />
            </div>
            <br />
            <button type="submit">Registrar</button>
            <a href="/api/sessions/auth/google" class="btn-google">Registrar con Google</a>
        </div>
    </form>
    <p>Â¿Ya tienes cuenta?</p><a href="/login">Ingresar</a>
</section>