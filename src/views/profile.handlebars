<section class="container">
    <h1>{{title}}</h1>
    <div>
        <p>Name: {{user.name}}</p>
        <p>Lastname: {{user.last_name}}</p>
        <p>Role: {{user.role}}</p>
        <p>Age: {{user.age}}</p>
        <p>Email: {{user.email}}</p>
        <form action="/api/sessions/logout" method="post">
            <button type="submit">Log Out</button>
        </form>
    </div>
</section>